{
  "name": "imagemia",
  "version": 2,
  "builds": [
    {
      "src": "web/**",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    { "source": "/", "destination": "/web/imagemia/index.html" },
    { "source": "/empresas", "destination": "/web/empresas/index.html" },
    { "source": "/empresas/petcimagenes", "destination": "/web/empresas/petcimagenes/index.html" }
  ]
}
