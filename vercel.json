{
  "version": 2,
  "builds": [
    {
      "src": "web/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/web/imagemia/index.html"
    },
    {
      "src": "/empresas",
      "dest": "/web/empresas/index.html"
    },
    {
      "src": "/empresas/petcimagenes",
      "dest": "/web/empresas/petcimagenes/index.html"
    },
    {
      "src": "/empresas/(.*)",
      "dest": "/web/empresas/$1"
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/web/$1" }
  ]
}
