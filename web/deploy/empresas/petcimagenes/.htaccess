# Deshabilitar cach√© completamente
<IfModule mod_headers.c>
    Header set Cache-Control "no-cache, no-store, must-revalidate, max-age=0"
    Header set Pragma "no-cache"
    Header set Expires "0"
</IfModule>

<FilesMatch "\.(html|htm|js|css|svg)$">
    FileETag None
    <IfModule mod_headers.c>
        Header unset ETag
        Header set Cache-Control "no-cache, no-store, must-revalidate, max-age=0"
    </IfModule>
</FilesMatch>
